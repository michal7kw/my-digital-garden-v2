{
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run start",
    "rewrites": [
        { "source": "/manifest.webmanifest", "destination": "/manifest.webmanifest" },
        { "source": "/(.*)", "destination": "/$1" }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}